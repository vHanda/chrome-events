{
  "name": "EventLog",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "An event log for chrome",
  "homepage_url": "http://vhanda.in/",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
        "src/types.js",
        "src/main.js"
    ]
  },
  "browser_action": {
      "default_title": "Tracker"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "default_locale": "en",
  "permissions": [
    "history",
    "idle",
    "tabs",
    "storage",
    "unlimitedStorage"
  ]
}
